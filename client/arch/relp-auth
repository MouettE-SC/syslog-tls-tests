       tls="on" tls.authmode="name" tls.permittedpeer="syslog"
       tls.cacert="/data/certs/pki/ca.crt"
       tls.mycert="/data/certs/pki/client.crt"
       tls.myprivkey="/data/certs/pki/client.key"
      )
